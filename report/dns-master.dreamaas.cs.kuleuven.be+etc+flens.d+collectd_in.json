 {
    "tags" : { "collectd" : "collectd"},
    "init": {
        "input" : {     
            "collectd-in" : {
                "type" :        "metric",
                "add-tag" :     "collectd"
            }
        },
        
        "filter" : {
            "collectd name to metric" :{
                "plugin" :      "template",
                "tags" :        ["collectd"],
                "template" :    "@{plugin!=type?plugin+'.':''}@{type}",
                "field" :       "metric"
            },
            "rename" : {
                "tags":         ["collectd"],
                "remove-tag" :  ["collectd"],
                "from":         ["type_instance", "plugin_instance"],
                "to":           ["type", "instance"]
            }
        }
    }
}