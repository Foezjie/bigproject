 {

    "name" : "dns-master",

    "tags" : { "tier" : "agent", "type" : "worker", "service" : "master"},
    "init": {
        "input" : {     
            "heartbeat":{
                "type":"metric",
                "interval":10000
            }
        },
        
        "filter" : {
            "tag":{}, 
            "json-encoder" : {
                "add-tag" : ["json"],
                "field" : "json",
                "fields" : ["time","source","metric","value","type","instance","temporalScope","message","tags","tenant"],
                "prio" : 1000
            }
        }
    }
}